PrettyPrinter {
    pretty = %:x !. -> x
    prettyInner = [pretty:x] -> x
    File = . prettyInner*:xs -> { xs }
    Namespace = .:name prettyInner*:xs -> { name " {\n" xs "}\n" }
    Rule = .:name prettyInner*:xs -> { "  " name " =\n" xs }
    Or = . (prettyInner:x -> {"    | " x "\n"})*:xs -> xs
    Scope = . -> "SCOPE"
}
